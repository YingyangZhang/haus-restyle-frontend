gzhang/Desktop/Porjects/Haus-Restyle/haus-restyle-frontend/src/components/Users/SignUp.js",[],[],"/Users/yingyangzhang/Desktop/Porjects/Haus-Restyle/haus-restyle-frontend/src/Components/ScrollRestoration.js",[],[],"/Users/yingyangzhang/Desktop/Porjects/Haus-Restyle/haus-restyle-frontend/src/Components/Users/Checkout.js",["286"],[],"import React from \"react\";\nimport { useNavigate } from \"react-router-dom\";\nimport Footer from \"../Footer/Footer\";\n\nexport default function Checkout({cart, setCart, isScrolled, user, setUser}) {\n    const subTotal = cart.reduce((a, c) => {\n        return a + (c.furniture.price * c.quantities);\n    }, 0);\n    const shipping = 50;\n    const tax = subTotal * .2;\n    const total = subTotal + shipping + tax;\n    const token = localStorage.getItem(\"jwt\");\n    const navigate = useNavigate();\n\n    function handleClick(e) {\n        e.preventDefault();\n\n        fetch('https://haus-db.onrender.com/orders', {\n            method: 'POST',\n            headers: {\n                'Content-Type': 'application/json',\n                Authorization: `Bearer ${token}`,\n            },\n            body: JSON.stringify({\n                'total_price': total,\n                'user_id': user.id,\n                'items': JSON.stringify(cart),\n            })\n        })\n        .then(r => r.json())\n        .then(data => {\n            setUser(data.user);\n            navigate('/thank_you');\n            \n            return fetch(`https://haus-db.onrender.com/users/clear_bag/${user.id}`, {\n                method: 'DELETE',\n                headers: {\n                    Authorization: `Bearer ${token}`\n                }\n            })\n        })\n        .then(r => r.json())\n        .then(data => {\n            setCart(data.user.cart_items);\n        })\n    }\n\n    return (\n        <div className='checkout-container flex-box'>\n            <div className={`empty-background grey-background ${isScrolled ? 'add-dropshadow' : ''}`}></div>\n\n            <div className='checkout'>\n            {cart.length !== 0 &&\n                <div className='checkout-order-container flex-box'>\n                    <div className=\"checkout-order\">\n                        <p className='checkout-headline'>Order Information</p>\n\n                        <div className='checkout-cards-container'>\n                            {cart.map(item => {\n                                return (\n                                    <div className='checkout-card-container flex-box' key={item.id}>\n                                        <div className='checkout-card flex-box'>\n                                            <div className='checkout-img-container'>\n                                                <img src={item.furniture.image.thumbnail} alt='image' className='img-position' />\n                                            </div>\n\n                                            <div className='checkout-card-info-container'>\n                                                <h1>{item.furniture.name}</h1>\n                                                <p>USD {item.furniture.price.toLocaleString()}</p>\n                                            </div>\n                                        </div>\n\n                                        <p>x{item.quantities}</p>\n                                    </div>\n                                )\n                            })}\n                        </div>\n\n                        <div className='checkout-summary-container'>\n                            <div className='checkout-summary'>\n                                <div className='flex-box'>\n                                    <p>Subtotal</p>\n                                    <p>USD {subTotal.toLocaleString()}</p>\n                                </div>\n\n                                <div className='flex-box'>\n                                    <p>Shipping</p>\n                                    <p>USD {shipping}</p>\n                                </div>\n\n                                <div className='flex-box'>\n                                    <p>Taxes</p>\n                                    <p>USD {tax.toLocaleString()}</p>\n                                </div>\n                            </div>\n\n                            <div className=\"checkout-total\">\n                                <div className=\"flex-box\">\n                                    <p>Total</p>\n                                    <p>USD {total.toLocaleString()}</p>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                    \n                    <div className=\"checkout-forms-container\">\n                        <form className='form-container checkout-form' >\n                            <h1 className='checkout-headline'>Shipping Information</h1>\n\n                            <div className='form-item-container'>\n                                <p>First Name</p>\n                                <input type='text' placeholder='Your first name' name='first_name' />\n                            </div>\n\n                            <div className='form-item-container'>\n                                <p>Last Name</p>\n                                <input type='text' placeholder='Your last name' name='last_name' />\n                            </div>\n\n                            <div className='form-item-container'>\n                                <p>Address</p>\n                                <input type='text' placeholder='Street address' name='last_name' />\n                                <input type='text' placeholder='Street address line 2' name='last_name' />\n\n                                <div className='form-items-group flex-box'>\n                                    <input type='text' placeholder='City' name='city' />\n                                    <input type='text' placeholder='State' name='state' />\n                                    <input type='text' placeholder='Zip Code' name='zip_code' />\n                                </div>\n                            </div>\n\n                            <div className='form-item-container'>\n                                <p>Phone</p>\n                                <input type='number' placeholder='Your phone number' name='last_name' />\n                            </div>\n                        </form>\n\n                        <form className='form-container checkout-form' >\n                            <h1 className='checkout-headline'>Payment Information</h1>\n\n                            <div className='form-item-container'>\n                                <p>Card Number</p>\n                                <input type='number' placeholder='Your card number' name='card_number' />\n                            </div>\n\n                            <div className='form-item-container'>\n                                <p>Cardholder Name</p>\n                                <input type='text' placeholder='Full name' name='full_name' />\n                            </div>\n\n                            <div className='form-items-group flex-box'>\n                                <div className='form-item-container'>\n                                    <p>Expiration Date</p>\n                                    <input type='number' placeholder='MM / YY' name='expiration_date' />\n                                </div>\n\n                                <div className='form-item-container'>\n                                    <p>Security Code</p>\n                                    <input type='number' placeholder='CVV' name='security_code' />\n                                </div>\n                            </div>\n\n                            <div className='form-buttons-container'>\n                                <button className='button' onClick={handleClick}> \n                                    <p>Finish and pay</p>\n                                    <i className='bx bx-arrow-back' ></i>\n                                </button>\n                            </div>\n                        </form>\n                    </div>\n                </div>\n            }\n            </div>\n            \n            <div className='subpages-footer-container'>\n                <Footer />\n            </div>\n        </div>\n    )\n}","/Users/yingyangzhang/Desktop/Porjects/Haus-Restyle/haus-restyle-frontend/src/Components/Home/Home.js",["287"],[],"import React, { useState } from \"react\";\nimport Footer from \"../Footer/Footer\";\n\nexport default function Home() {\n    const [randomImgIndex, setrandomImgIndex] = useState(randomIndex(0, 2))\n    const backgroundImg = ['https://haus-imgs.s3.amazonaws.com/backgroud_01.png', 'https://haus-imgs.s3.amazonaws.com/backgroud_02.png', 'https://haus-imgs.s3.amazonaws.com/backgroud_03.png'];\n\n    function randomIndex(min, max) {\n        return Math.floor(Math.random() * (max - min + 1) + min);\n    }   \n\n    const homeBackground = {\n        backgroundImage: `url(${backgroundImg[randomImgIndex]})`\n    };\n\n    return (\n        <div className='home-container' style={homeBackground}>\n            <div className='home-headline-container'>\n                <h1>'Make a Statement'</h1>\n                \n                <p>Experience the art of furniture.</p>\n            </div>\n            \n            <div className='home-footer-container'>\n                <Footer />\n            </div>\n        </div>\n    )\n}","/Users/yingyangzhang/Desktop/Porjects/Haus-Restyle/haus-restyle-frontend/src/Components/Furnitures/Inspect.js",["288"],[],"import { React, useEffect, useState } from \"react\";\nimport { useParams } from \"react-router-dom\";\nimport RelatedFurnitures from \"./RelatedFurnitures\";\nimport LoadingScreen from \"../Loading/LoadingScreen\";\nimport Footer from \"../Footer/Footer\";\n\nexport default function Inspect({isScrolled, user, setUser, cart, setCart}) {\n    const [furniture, setFurniture] = useState({});\n    const [relatedFurnitures, setRelatedFurnitures] = useState([]);\n    const { id } = useParams();\n    const [isScrollingDown, setIsScrollingDown] = useState(false);\n    const [isLoading, setIsLoading] = useState(true);\n    const token = localStorage.getItem(\"jwt\");\n\n    useEffect(() => {\n        const handleScroll = () => {\n            const scrollPosition = window.pageYOffset;\n            const scrollThreshold = 83;\n            \n            if (scrollPosition > scrollThreshold && !isScrollingDown) {\n                setIsScrollingDown(true);\n            } else if (scrollPosition < scrollThreshold && isScrollingDown) {\n                setIsScrollingDown(false);\n            }\n        };\n\n        window.addEventListener('scroll', handleScroll);\n\n        return () => {\n            window.removeEventListener('scroll', handleScroll);\n        };\n    }, [isScrollingDown]);\n\n    useEffect(() => {\n        setIsLoading(true);\n\n        fetch(`https://haus-db.onrender.com/furnitures/${id}`)\n        .then(r => r.json())\n        .then(data => {\n            setFurniture(data);\n            setIsLoading(false);\n        })\n    },[id]);\n\n    useEffect(() => {\n        fetch('https://haus-db.onrender.com/furnitures')\n          .then(r => r.json())\n          .then(data => {\n            const filteredFurnitures = data.filter(relatedFurniture => {\n              return relatedFurniture.category.category_name === furniture.category?.category_name;\n            });\n            const randomFurnitures = filteredFurnitures.sort(() => Math.random() - 0.5).slice(0, 3);\n            setRelatedFurnitures(randomFurnitures);\n          });\n    }, [furniture.category]);\n\n    function handleAddToCart() {\n        const alreadyInCart = cart.find(item => item.furniture.name === furniture.name);\n        if (alreadyInCart) {\n            if (alreadyInCart.quantities <= 9) {\n                fetch(`https://haus-db.onrender.com/cart_items/${alreadyInCart.id}`, {\n                    method: 'PATCH',\n                    headers: {\n                        'Content-Type': 'application/json',\n                        Authorization: `Bearer ${token}`,\n                    },\n                    body: JSON.stringify({\n                        'quantities': alreadyInCart.quantities + 1,\n                    })\n                }).then(r => r.json())\n                .then(data => {\n                    setCart(data.user.cart_items)\n                })\n            }\n        } else {\n            fetch('https://haus-db.onrender.com/cart_items/', {\n                method: 'POST',\n                headers: {\n                    'Content-Type': 'application/json',\n                    Authorization: `Bearer ${token}`,\n                },\n                body: JSON.stringify({\n                    'quantities': 1,\n                    'user_id': user.id,\n                    'furniture_id': furniture.id,\n                })\n            }).then(r => r.json())\n            .then(data => {\n                setUser(data.user)\n                setCart(data.user.cart_items)\n            })\n        }\n    }\n\n    return ( \n    <div className='inspect-container'>\n        {isLoading && <LoadingScreen />}\n\n        <div className={`inspect-header-container flex-box grey-background ${isScrolled ? 'add-dropshadow' : ''}`}>\n            <div className={`inspect-header-left flex-box ${isScrollingDown ? 'shrink-container' : ''}`}>\n                <div className='inspect-furniture'>\n                    <h1 className={furniture.name && furniture.name.length > 13 ? 'small-font' : 'big-font'}>\n                        {furniture.name}\n                    </h1>\n\n                    <p className={isScrollingDown ? 'hide-price' : ''}>USD {furniture.price && furniture.price.toLocaleString()}</p>\n                </div>\n\n                <div className='add-to-cart-button flex-box' onClick={handleAddToCart} >\n                    <i className='bx bx-plus'></i>\n\n                    <p>Add to Cart</p>\n                </div>\n            </div>\n\n            <div className={`inspect-header-right ${isScrollingDown ? 'hide-designer' : ''}`}>\n                <p>{furniture.designer}</p>\n            </div>\n        </div>\n\n        <div className='inspect-imgs-container flex-box'>\n            {furniture.image && \n                Object.values(furniture.image).slice(0, 3).map(src => {\n                    return (\n                        <div className='inspect-img-container' key={src}>\n                            <img src={src} alt='image' className='fixed-img'/>\n                        </div>\n                    )\n                })\n            }\n        </div>\n        \n        <div className='inspect-details-container'>\n            <div className='inspect-detail flex-box'>\n                <p>Material</p>\n\n                <p>{furniture.material}</p>\n            </div>\n\n            <div className='inspect-detail flex-box'>\n                <p>Dimensions</p>\n\n                <p>{furniture.dimensions}</p>\n            </div>\n\n            <div className='inspect-detail flex-box'>\n                <p>Origin</p>\n\n                <p>{furniture.origin}</p>\n            </div>\n        </div>\n\n        <div className='related-furnitures-container'>\n            <h1>Related Furnitures</h1>\n            \n            {relatedFurnitures && <RelatedFurnitures relatedFurnitures={relatedFurnitures} />}\n        </div>\n\n        <div className='subpages-footer-container'>\n            <Footer />\n        </div>\n\n        \n    </div>\n    )\n    \n      \n}","/Users/yingyangzhang/Desktop/Porjects/Haus-Restyle/haus-restyle-frontend/src/Components/Header/Header.js",[],[],"/Users/yingyangzhang/Desktop/Porjects/Haus-Restyle/haus-restyle-frontend/src/Components/Furnitures/Furnitures.js",["289"],[],"import { React, useState, useEffect } from \"react\";\nimport { useNavigate } from \"react-router-dom\";\nimport { Link } from 'react-scroll';\nimport Search from \"./Search\";\nimport LoadingScreen from \"../Loading/LoadingScreen\";\nimport Footer from \"../Footer/Footer\";\n\nexport default function Furnitures({furnitures, isFurnituresLoading, setFurnitures, searchResult, setSearchResult, isScrolled}) {\n    const [selectedCat, setSelectedCat] = useState('All');\n    const [isSearch, setIsSearch] = useState(false);\n    const filteredFurnitures = searchResult.filter(furniture => {\n        return selectedCat === \"All\" ? furniture : furniture.category.category_name === selectedCat;\n    });\n    \n    const navigate = useNavigate();\n\n    useEffect(() => {\n        const handlePopstate = () => {\n          window.location.reload();\n        };\n      \n        window.addEventListener('popstate', handlePopstate);\n      \n        return () => {\n          window.removeEventListener('popstate', handlePopstate);\n        };\n    }, []);\n\n    function handleCat(e) {\n        setSelectedCat(e.target.value);\n    }\n\n    return (\n        <div className='furnitures-container flex-box' id='back-to-top'>\n            {isFurnituresLoading && <LoadingScreen />}\n\n            <div className={`furnitures-menu-container flex-box grey-background ${isScrolled ? 'add-dropshadow' : ''}`} >\n                <div className=\"category-container flex-box\">\n                    <label className=\"category-selection\">\n                        <input type=\"radio\" name='selection' value=\"All\" defaultChecked={true} onChange={handleCat} />\n\n                        <span>All</span>\n                    </label>\n\n                    <label className=\"category-selection\">\n                        <input type=\"radio\" name='selection' value=\"Chair\" onChange={handleCat} />\n\n                        <span>Chair</span>\n                    </label>\n\n                    <label className=\"category-selection\">\n                        <input type=\"radio\" name='selection' value=\"Table\" onChange={handleCat} />\n\n                        <span>Table</span>\n                    </label>\n\n                    <label className=\"category-selection\">\n                        <input type=\"radio\" name='selection' value=\"Sofa\" onChange={handleCat} />\n\n                        <span>Sofa</span>\n                    </label>\n                </div>\n\n                <i className='bx bx-search' onClick={() => setIsSearch(true)}></i>\n            </div>\n\n            <div className='cards-container'>\n                {filteredFurnitures.map(furniture => {\n                    return (\n                        <div className='card' onClick={() => navigate(`/furnitures/${furniture.id}`)} key={furniture.id}>\n                            <div className='card-img-container'>\n                                <img src={furniture.image.thumbnail} alt='image' className='fixed-img' />\n                            </div>\n                            \n                            <div className='card-info-container flex-box'>\n                                <p>{furniture.designer}</p>\n\n                                <p>{furniture.name}</p>\n                            </div>\n                        </div>\n                    )\n                })}\n            </div>\n\n            <Link to=\"back-to-top\"\n                smooth={true}\n                duration={500}\n                offset={-100} \n                className='back-to-top-button'>\n                <i className='bx bx-chevron-up'></i>\n            </Link>\n\n            <div className='subpages-footer-container'>\n                <Footer />\n            </div>\n\n            {isSearch ? \n            <Search setIsSearch={setIsSearch} \n                    furnitures={furnitures} \n                    setFurnitures={setFurnitures} \n                    setSearchResult={setSearchResult}/> \n            : ''}\n        </div>\n    )\n}","/Users/yingyangzhang/Desktop/Porjects/Haus-Restyle/haus-restyle-frontend/src/Components/Users/Forms.js",[],[],"/Users/yingyangzhang/Desktop/Porjects/Haus-Restyle/haus-restyle-frontend/src/Components/Users/Cart.js",["290"],[],"import React from \"react\";\nimport { useNavigate } from \"react-router-dom\";\nimport Footer from \"../Footer/Footer\";\n\nexport default function Cart({cart, setCart, isScrolled}) {\n    const navigate = useNavigate();\n    const token = localStorage.getItem(\"jwt\");\n    const total = cart.reduce((a, c) => {\n        return a + (c.furniture.price * c.quantities);\n    }, 0);\n\n    function handleAdd(id, quantities) {\n        if (quantities <= 9) {\n            fetch(`https://haus-db.onrender.com/cart_items/${id}`, {\n                method: 'PATCH',\n                headers: {\n                    'Content-Type': 'application/json',\n                    Authorization: `Bearer ${token}`,\n                },\n                body: JSON.stringify({\n                    'quantities': quantities + 1,\n                })\n            })\n            .then(r => r.json())\n            .then(data => {\n                const updatedCart = [...cart];\n                const index = updatedCart.findIndex(item => item.id === id);\n                updatedCart[index] = data.user.cart_items.find(item => item.id === id);\n                setCart(updatedCart);\n            })\n        }\n    }\n\n    function handleMinus(id, quantities) {\n        if( quantities > 1) {\n            fetch(`https://haus-db.onrender.com/cart_items/${id}`, {\n                method: 'PATCH',\n                headers: {\n                    'Content-Type': 'application/json',\n                    Authorization: `Bearer ${token}`,\n                },\n                body: JSON.stringify({\n                    'quantities': quantities - 1,\n                })\n            }).then(r => r.json())\n            .then(data => {\n                const updatedCart = [...cart];\n                const index = updatedCart.findIndex(item => item.id === id);\n                updatedCart[index] = data.user.cart_items.find(item => item.id === id);\n                setCart(updatedCart);\n            })\n        }\n    }\n\n    function handleRemove(id) {\n        fetch(`https://haus-db.onrender.com/cart_items/${id}`, {\n            method: 'DELETE',\n            headers: {\n                Authorization: `Bearer ${token}`,\n            }\n        }).then(r => r.json())\n        .then(data => {\n            setCart(data.user.cart_items);\n        })\n    }\n   \n    return (\n        <div className='cart-container flex-box'>\n            <div className={`empty-background grey-background ${isScrolled ? 'add-dropshadow' : ''}`}></div>    \n\n            {cart.length !== 0 ?\n            <div className='cart flex-box'>\n                <div className='cart-cards-container flex-box'>\n                    <h1>Order Summary</h1>\n                    {cart.map(item => {\n                        return (\n                            <div className='cart-card flex-box' key={item.id}>\n                                <div className='cart-img-container' onClick={() => navigate(`/furnitures/${item.furniture.id}`)}>\n                                    <img src={item.furniture.image.thumbnail} alt='image' className=\"fixed-img\" />\n                                </div>\n\n                                <div className=\"cart-card-info flex-box\">\n                                    <div className='cart-card-info-container'>\n                                        <h1 onClick={() => navigate(`/furnitures/${item.furniture.id}`)}>\n                                            {item.furniture.name}\n                                        </h1>\n                                        <p>USD {item.furniture.price.toLocaleString()}</p>\n                                    </div>\n\n                                    <div className='cart-card-operations-container flex-box'>\n                                        <div className='quantity-operation flex-box'>\n                                            <p>Quantity</p>\n\n                                            <div className='quantity-buttons flex-box'>\n                                                <i className='bx bx-plus' onClick={() => handleAdd(item.id, item.quantities)}></i>\n                                                <p>{item.quantities}</p>\n                                                <i className='bx bx-minus' onClick={() => handleMinus(item.id, item.quantities)}></i>\n                                            </div>\n                                        </div>\n\n                                    <div className='remove-button' onClick={() => handleRemove(item.id)}>\n                                        <p>Remove Furniture</p>\n                                    </div>\n                                </div>\n                            </div>\n                        </div> \n                    )})}   \n                </div>\n                    \n                <div className='cart-total-container for-small-screen'>\n                    <div className=\"flex-box\">\n                        <p>Delivery</p>\n                        <p>Excluding delivery</p>\n                    </div>\n\n                    <div className='flex-box'>\n                        <p>Total(Excel. tax)</p>\n                        <p>USD {total.toLocaleString()}</p>\n                    </div>\n\n                    <button className='button' onClick={() => navigate('/checkout')}> \n                        <p>Continue to checkout</p>\n                        <i className='bx bx-arrow-back' ></i>\n                    </button>\n                </div>\n            </div>\n            :\n            <p className=\"empty-cart-message\">Your cart is empty.</p> }\n\n            <div className='subpages-footer-container'>\n                <Footer />\n            </div>\n        </div>\n    )\n}","/Users/yingyangzhang/Desktop/Porjects/Haus-Restyle/haus-restyle-frontend/src/Components/Users/ThankYou.js",[],[],"/Users/yingyangzhang/Desktop/Porjects/Haus-Restyle/haus-restyle-frontend/src/Components/Users/Profile.js",[],[],"/Users/yingyangzhang/Desktop/Porjects/Haus-Restyle/haus-restyle-frontend/src/Components/Furnitures/RelatedFurnitures.js",["291"],[],"/Users/yingyangzhang/Desktop/Porjects/Haus-Restyle/haus-restyle-frontend/src/Components/Users/SignUp.js",[],[],"/Users/yingyangzhang/Desktop/Porjects/Haus-Restyle/haus-restyle-frontend/src/Components/Users/SignIn.js",[],[],"/Users/yingyangzhang/Desktop/Porjects/Haus-Restyle/haus-restyle-frontend/src/Components/Furnitures/Search.js",[],[],"/Users/yingyangzhang/Desktop/Porjects/Haus-Restyle/haus-restyle-frontend/src/Components/Loading/LoadingScreen.js",[],[],"/Users/yingyangzhang/Desktop/Porjects/Haus-Restyle/haus-restyle-frontend/src/Components/Footer/Footer.js",["292"],[],{"ruleId":"293","severity":1,"message":"294","line":40,"column":5,"nodeType":"295","endLine":40,"endColumn":7,"suggestions":"296"},{"ruleId":"293","severity":1,"message":"294","line":40,"column":5,"nodeType":"295","endLine":40,"endColumn":7,"suggestions":"297"},{"ruleId":"298","severity":1,"message":"299","line":5,"column":28,"nodeType":"300","messageId":"301","endLine":5,"endColumn":45},{"ruleId":"302","severity":1,"message":"303","line":8,"column":13,"nodeType":"304","messageId":"305","endLine":8,"endColumn":66,"fix":"306"},{"ruleId":"307","severity":1,"message":"308","line":126,"column":29,"nodeType":"304","endLine":126,"endColumn":79},{"ruleId":"307","severity":1,"message":"308","line":72,"column":33,"nodeType":"304","endLine":72,"endColumn":106},{"ruleId":"307","severity":1,"message":"308","line":17,"column":29,"nodeType":"304","endLine":17,"endColumn":108},{"ruleId":"307",